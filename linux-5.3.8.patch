+  
+  for (i = 0; i < gvlimit; i++)
+    {
+      fb_ptr[i*ghlimit + i] = 3;
+    }
+  
+  for (i = 0; i < gvlimit; i++)
+    {
+      int j;
+      for (j = 0; j < ghlimit; j += 100)
+        {
+          fb_ptr[i*ghlimit + j] = 1;
+        }
+      fb_ptr[(i+1)*ghlimit - 1] = 1;
+    }
+}
+#endif

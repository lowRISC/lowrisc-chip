+{
+    vc->vc_can_do_color = 1;
+    if (init) {
+	vc->vc_cols = LOWRISC_COLUMNS;
+	vc->vc_rows = LOWRISC_ROWS;
+    } else
+	vc_resize(vc, LOWRISC_COLUMNS, LOWRISC_ROWS);
+    
+}

+static void lowrisc_sd_set_led(struct lowrisc_sd_host *host, unsigned char state)
+{
+  volatile uint64_t *sd_base = host->ioaddr;
+  if (!led_sd_base)
+    led_sd_base = sd_base;
+  if (state)
+    led_last |= red_led;
+  else
+    led_last &= ~red_led;
+  led_sd_base[led_reg] = led_last;
+}

+static void lowrisc_fbcon_clear(struct vc_data *vc, int sy, int sx, int height, int width)
+{
+  int y;
+  for (y = sy; y < height; y++)
+    {
+      myset(hid_fb_ptr+y*LOWRISC_COLUMNS+sx, 0, width);
+    }
+}
